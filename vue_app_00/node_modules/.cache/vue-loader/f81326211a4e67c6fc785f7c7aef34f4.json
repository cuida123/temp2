\r\n    <div class=\"container\" v-html=\"newinfo.content\">\r\n    </div>\r\n    <!--评论区域-->\r\n    <comment-box :id=\"this.id\"></comment-box>\r\n</div>\r\n</template>\r\n<script>\r\n    import comment from\"../subcomponents/comment\"\r\n    export default{\r\n        data(){\r\n            return{\r\n                id:this.$route.params.id,\r\n                newinfo:{}\r\n            }\r\n        },\r\n        methods:{\r\n            getNewsDetail(){\r\n                //获取新闻详细信息\r\n\r\n                var url=\"newslist/find?id=\"+this.id;\r\n                this.$http.get(url).then(result=>{\r\n                    this.newinfo=result.body.msg[0];\r\n                    console.log(this.newinfo);\r\n                })\r\n            },\r\n        },\r\n        created(){\r\n            console.log(this.$route.params.id);\r\n            this.getNewsDetail();\r\n        },\r\n        components:{\r\n            'comment-box':comment   //注册子组件\r\n        }\r\n    }\r\n</script>\r\n<style >\r\n    .app_newsdetail{\r\n        padding:0 4px;\r\n    }\r\n    .app_newsdetail .title{\r\n        font-size:16px;\r\n        text-align:center;\r\n        color:red;\r\n        margin:15px 0;\r\n    }\r\n    .app_newsdetail .subtitle{\r\n        font-size:13px;\r\n        color:#226aff;\r\n        display:flex;\r\n        justify-content:space-between;\r\n    }\r\n    .app_newsdetail .container img{\r\n        width:100%;\r\n    }\r\n</style>"]}]}